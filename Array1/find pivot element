class Solution {
public:
    int pivotIndex(vector<int>& nums) {
        int left=0;
        int n=nums.size();
        int right=n-1;
        while(left<=right){
          int tempLeftSum = nums[left] + ((left + 1 < n) ? nums[left + 1] : 0);
            int tempRightSum = nums[right] + ((right - 1 >= 0) ? nums[right - 1] : 0);
        left++;
           right--;
        }
return -1;
    }
};
