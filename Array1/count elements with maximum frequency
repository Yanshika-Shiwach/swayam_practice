class Solution {
public:
    int maxFrequencyElements(vector<int>& nums) {
        int n=nums.size();

        unordered_map<int,int>freq;
        int frequency=0,cnt=0;
        for(int i=0;i<n;i++){
            freq[nums[i]]++;
        }
        for(auto&pair :freq){
            if(pair.second==frequency)
            cnt++;
            else if(pair.second>frequency){
            frequency=pair.second;
            cnt=1;
        }}
        return frequency*cnt;
    }
};
