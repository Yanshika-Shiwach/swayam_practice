class Solution {
public:
    string removeDuplicates(const string& s) {
        unordered_set<char> seen;
        string result;
        for (char c : s) {
            if (seen.find(c) == seen.end()) {
                result.push_back(c);
                seen.insert(c);
            }
        }
        return result;
    }
};
