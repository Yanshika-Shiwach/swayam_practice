class Solution {
public:
    bool isValid(string s) {
        stack<char> stk;
        unordered_map<char, char> mapping = {
            {')', '('},
            {'}', '{'},
            {']', '['}
        };

        for (char c : s) {
            if (mapping.count(c) == 0) {
                 stk.push(c);
            } else {
                if (stk.empty() || stk.top() != mapping[c])
                    return false;
                stk.pop();
            }
        }
        return stk.empty();
    }
};
